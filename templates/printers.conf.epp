<% $::ksmb::queues.each |$queue| { %>
  <% if $queue['default'] == true { %>
<DefaultPrinter <%= $queue['name'] -%>>
  <% } else { %>
<Printer <%= $queue['name'] -%>>
  <% } %>
DeviceURI <%= $queue['uri'] %>
Accepting Yes
Shared No
JobSheets none none
QuotaPeriod 0
PageLimit 0
KLimit 0
OpPolicy default
ErrorPolicy stop-printer
</Printer>
<% } %>